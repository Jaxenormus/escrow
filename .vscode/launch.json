{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TS Node",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "sh",
      "program": "node_modules/.bin/ts-node",
      "args": ["-r", "tsconfig-paths/register", "-T", "src/index.ts"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnFirstSessionStart"
    }
  ]
}
