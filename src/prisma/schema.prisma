generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Address {
  id       String @id
  data     String
  public   String
  private  String
  recovery String
}

model Job {
  id      String @id
  channelId String

  @@index([channelId])
}
